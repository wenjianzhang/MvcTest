@model MvcTest.Models.Position

@{
    ViewBag.Title = "Details";
}

@section HeadContent{
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Content/css/bootstrap-responsive.min.css" />
    <link rel="stylesheet" href="~/Content/css/colorpicker.css" />
    <link rel="stylesheet" href="~/Content/css/datepicker.css" />
    <link rel="stylesheet" href="~/Content/css/uniform.css" />
    <link rel="stylesheet" href="~/Content/css/select2.css" />
    <link rel="stylesheet" href="~/Content/css/matrix-style.css" />
    <link rel="stylesheet" href="~/Content/css/matrix-media.css" />
    <link rel="stylesheet" href="~/Content/css/bootstrap-wysihtml5.css" />
    <link href="~/fonts/css/font-awesome.css" rel="stylesheet" />
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,700,800' rel='stylesheet' type='text/css'>
}

<div id="content">
    <div id="content-header">
        <div id="breadcrumb"> <a href="index.html" title="Go to Home" class="tip-bottom"><i class="icon-home"></i> Home</a> <a href="#" class="tip-bottom">职位表</a> <a href="#" class="current">详情</a> </div>
        @*<h1>Common Form Elements</h1>*@
    </div>
    <div class="container-fluid">
        <div class="row-fluid">
            <div class="span12">
                <div class="widget-box">
                    <div class="widget-title">
                        <span class="icon"> <i class="icon-align-justify"></i> </span>
                        <h5>职位表</h5>
                    </div>
                    <div class="widget-content nopadding">


                        @using (Html.BeginForm("", "", FormMethod.Post, new { @class = "form-horizontal" }))
                        {

                            @Html.AntiForgeryToken()
                            <div class="control-group">
                                @Html.LabelFor(model => model.Name, "职位名称", new { @class = "control-label" })
                                <div class="controls">
                                    @Html.TextBoxFor(model => model.Name, new { @class = "span8", placeholder = "职位名称", @disabled = "" })
                                    @Html.ValidationMessageFor(model => model.Name)
                                </div>
                            </div>
                            <div class="control-group">
                                @Html.LabelFor(model => model.Branch_Id, "部门自编号ID", new { @class = "control-label" })
                                <div class="controls">
                                    @Html.TextBoxFor(model => model.Branch_Id, new { @class = "span8", placeholder = "部门自编号ID", @disabled = "" })
                                    @Html.ValidationMessageFor(model => model.Branch_Id)
                                </div>
                            </div>
                            <div class="control-group">
                                @Html.LabelFor(model => model.Branch_Code, "部门编号", new { @class = "control-label" })
                                <div class="controls">
                                    @Html.TextBoxFor(model => model.Branch_Code, new { @class = "span8", placeholder = "部门编号", @disabled = "" })
                                    @Html.ValidationMessageFor(model => model.Branch_Code)
                                </div>
                            </div>
                            <div class="control-group">
                                @Html.LabelFor(model => model.Branch_Name, "部门名称", new { @class = "control-label" })
                                <div class="controls">
                                    @Html.TextBoxFor(model => model.Branch_Name, new { @class = "span8", placeholder = "部门名称", @disabled = "" })
                                    @Html.ValidationMessageFor(model => model.Branch_Name)
                                </div>
                            </div>
                            <div class="control-group">
                                @Html.LabelFor(model => model.PagePower, "菜单操作权限，有操作权限的菜单ID列表：|1|2|3|4|5| ", new { @class = "control-label" })
                                <div class="controls">
                                    @Html.TextBoxFor(model => model.PagePower, new { @class = "span8", placeholder = "菜单操作权限，有操作权限的菜单ID列表：|1|2|3|4|5|", @disabled = "" })
                                    @Html.ValidationMessageFor(model => model.PagePower)
                                </div>
                            </div>
                            <div class="control-group">
                                @Html.LabelFor(model => model.ControlPower, "页面功能操作权限，各个页面有操作权限的菜单ID和页面权限标志ID列表：|1,1|2,1|2,2|2,4|", new { @class = "control-label" })
                                <div class="controls">
                                    @Html.TextBoxFor(model => model.ControlPower, new { @class = "span8", placeholder = "页面功能操作权限，各个页面有操作权限的菜单ID和页面权限标志ID列表：|1,1|2,1|2,2|2,4|", @disabled = "" })
                                    @Html.ValidationMessageFor(model => model.ControlPower)
                                </div>
                            </div>
                            <div class="control-group">
                                @Html.LabelFor(model => model.IsSetBranchPower, "是否有操作绑定指定部门的权限，0=无，1=有   ", new { @class = "control-label" })
                                <div class="controls">
                                    @Html.TextBoxFor(model => model.IsSetBranchPower, new { @class = "span8", placeholder = "是否有操作绑定指定部门的权限，0=无，1=有   ", @disabled = "" })
                                    @Html.ValidationMessageFor(model => model.IsSetBranchPower)
                                </div>
                            </div>
                            <div class="control-group">
                                @Html.LabelFor(model => model.SetBranchCode, "绑定部门的编号   ", new { @class = "control-label" })
                                <div class="controls">
                                    @Html.TextBoxFor(model => model.SetBranchCode, new { @class = "span8", placeholder = "绑定部门的编号 ", @disabled = "" })
                                    @Html.ValidationMessageFor(model => model.SetBranchCode)
                                </div>
                            </div>
                            <div class="control-group">
                                @Html.LabelFor(model => model.Manager_Id, "修改人员id   ", new { @class = "control-label" })
                                <div class="controls">
                                    @Html.TextBoxFor(model => model.Manager_Id, new { @class = "span8", placeholder = "修改人员id   ", @disabled = "" })
                                    @Html.ValidationMessageFor(model => model.Manager_Id)
                                </div>
                            </div>
                            <div class="control-group">
                                @Html.LabelFor(model => model.Manager_CName, "修改人员姓名   ", new { @class = "control-label" })
                                <div class="controls">
                                    @Html.TextBoxFor(model => model.Manager_CName, new { @class = "span8", placeholder = "修改人员姓名   ", @disabled = "" })
                                    @Html.ValidationMessageFor(model => model.Manager_CName)
                                </div>
                            </div>
                            <div class="control-group">
                                @Html.LabelFor(model => model.UpdateDate, "修改时间   ", new { @class = "control-label" })
                                <div class="controls">
                                    @Html.TextBoxFor(model => model.UpdateDate, new { @class = "span8", placeholder = "修改时间   ", @disabled = "" })
                                    @Html.ValidationMessageFor(model => model.UpdateDate)
                                </div>
                            </div>

                        }
                        <div class="form-actions">
                            @Html.ActionLink("返回", "Index", "WebConfig", new { @class = "btn btn-success" })
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section FootContent{
    <script src="~/Scripts/js/jquery.min.js"></script>
    <script src="~/Scripts/js/matrix.js"></script>
    <script src="~/Scripts/js/jquery.ui.custom.js"></script>
    <script src="~/Scripts/js/bootstrap.min.js"></script>
    <script src="~/Scripts/js/bootstrap-datepicker.js"></script>
    <script src="~/Scripts/js/masked.js"></script>
    <script src="~/Scripts/js/jquery.uniform.js"></script>
    <script src="~/Scripts/js/select2.min.js"></script>

    @*<script src="~/Scripts/js/matrix.form_common.js"></script>*@
    <script src="~/Scripts/js/wysihtml5-0.3.0.js"></script>
    <script src="~/Scripts/js/jquery.peity.min.js"></script>
    <script src="~/Scripts/js/bootstrap-wysihtml5.js"></script>
    <script>
        $('.textarea_editor').wysihtml5();
    </script>
}
