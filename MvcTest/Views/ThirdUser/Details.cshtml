<div id="content">
    <div id="content-header">
        <div id="breadcrumb"> <a href="index.html" title="Go to Home" class="tip-bottom"><i class="icon-home"></i> Home</a> <a href="#" class="tip-bottom">User</a> <a href="#" class="current">详情</a> </div>
        @*<h1>Common Form Elements</h1>*@
    </div>
